{
  "version": 2,
  "builds": [
    {
      "src": "app.py",  # Change to the main Python file of your project
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"  # This should point to your main Python file
    }
  ],
  "env": {
    "MISTRAL_API_KEY": "@mistral-api-key"  # Replace with your Vercel secret or API key if needed
  }
}
